<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="680.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="680.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
              <buttons>
                  <Button fx:id="newTransferButton" mnemonicParsing="false" onAction="#newTransferPressed" text="New Transfer" />
                  <Button fx:id="newExpenseButton" mnemonicParsing="false" onAction="#newExpensePressed" text="New Expense" />
                  <Button fx:id="newIncomeButton" mnemonicParsing="false" onAction="#newIncomePressed" text="New Income" />
              </buttons>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </VBox.margin>
            </ButtonBar>
            <TableView fx:id="transactionTable" prefHeight="644.0" prefWidth="900.0">
              <columns>
                <TableColumn fx:id="categoryColumn" prefWidth="75.0" text="Category" />
                <TableColumn fx:id="amountColumn" prefWidth="75.0" text="Amount" />
                <TableColumn fx:id="typeColumn" prefWidth="75.0" text="Type" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
